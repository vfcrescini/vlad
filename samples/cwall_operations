/* query 1 */
seq add Request_Access_Company1(S, Object1);
query holds(S, Accessible, Object2);

/* list & clear sequences */
seq list;
seq del 0;

/* query 2 */
seq add Request_Access_Company2(S, Object2);
query holds(S, Accessible, Object1);
