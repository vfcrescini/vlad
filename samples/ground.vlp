 */

entity sub ss0, ss1, ss2;
entity acc as0, as1, as2;
entity obj os0, os1, os2;

entity sub-grp sg0, sg1, sg2;
entity acc-grp ag0, ag1, ag2;
entity obj-grp og0, og1, og2;

initially holds(sg0, ag0, og0);
initially memb(ss0, sg0);

always memb(ss0, sg0);
always memb(as0, AG0);
always holds(SS0, AS0, os0) with absence !holds(SS0, AS0, os0);

f1(SS0, AS0, OG0)
  causes holds(SS0, AS0, OG0) && memb(SS0, SG0);
