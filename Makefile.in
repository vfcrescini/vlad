DIRS = common kb parser main

all :
	@set -e && for i in $(DIRS); do \
	$(MAKE) -C $$i all;             \
	done

clean :
	@set -e && for i in $(DIRS); do \
	$(MAKE) -C $$i clean;           \
	done

distclean :
	@set -e && for i in $(DIRS); do \
	$(MAKE) -C $$i distclean;       \
	done
	$(RM) -r include lib Makefile config.status config.log autom4te.cache
