DIRS = lib main
TOPSRCDIR = @top_srcdir@
DISTDIR = @DISTDIR@

all :
	@set -e && for i in $(DIRS); do \
	$(MAKE) -C $$i all;             \
	done

install :
	@set -e && for i in $(DIRS); do \
        $(MAKE) -C $$i install;         \
        done

uninstall :
	@set -e && for i in $(DIRS); do \
        $(MAKE) -C $$i uninstall;       \
        done

clean :
	@set -e && for i in $(DIRS); do \
	$(MAKE) -C $$i clean;           \
	done

distclean :
	@set -e && for i in $(DIRS); do \
	$(MAKE) -C $$i distclean;       \
	done
	$(RM) -r $(DISTDIR) Makefile config.h config.status config.log autom4te.cache
